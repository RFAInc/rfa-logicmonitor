            <name>scriptgroovy</name>
            <value>import com.santaba.agent.groovyapi.win32.WMI

// Set hostname variable.
def hostname = hostProps.get(&#34;system.hostname&#34;);

// Try the following code.
try
{
    // This is our namespace, we&#39;re using the default value here to illustrate
    // how to pass it if your object isn&#39;t in root\cimv2
    // This parameter is optional.
    def namespace = &#34;CIMv2&#34;

    // These are our WMI queries. We limit what we get back from the _Processor class
    // to just the property totals.  The _System class returns only the aggregated property results,
    // so we don&#39;t need to add the &#39;where Name = &#39; clause to it

    String[] wmi_classes = [ &#39;Win32_PerfRawData_Tcpip_UDPv4&#39;, &#39;Win32_PerfRawData_Tcpip_TCPv4&#39;]

    // Where we&#39;ll put the resultant query objects
    def wmi_query_results = []

    // You can also pass an optional timeout value, the default is 30 seconds
    def timeout = 30

    // instantiate the WMI class objects and put them in the list object to reference later
    wmi_classes.each
    { klass -&#62;
        wmi_query_results &#60;&#60; WMI.queryAll(hostname, namespace, &#39;select * from &#39; + &#34;${klass}&#34;, timeout)
    }

    // wmi_query_results is a list of 2 list objects (one for each query),
    // each containing a single map of the properties names
    // as the key and the associated values as the value.


    // Print out the output of our WMI query.
    wmi_query_results.each
    { wmi_query_output -&#62;
        wmi_query_output[0].each {
            key, value -&#62;
                println &#34;${key}=${value}&#34;

        }

    }

}

// Catch any exceptions that may have occurred.
catch (Exception e)
{
    // Print exception out.
    println e
    return 1
}

// Exit by returning 0.
return 0</value>
