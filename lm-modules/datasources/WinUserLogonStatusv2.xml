            <name>scriptgroovy</name>
            <value>function Get-UserSessions {
    $UserSessions = $(&#38; &#34;$env:windir\system32\QWinSta.exe&#34;) | ForEach-Object {$_.substring(19)} | ConvertFrom-String -PropertyNames User, ID, State | Where-Object {$_.User -ne &#34;&#34;} | Where-Object {$_.User -ne &#34;USERNAME&#34;} | Select-Object User, State    
    Write-Output $UserSessions
}

$result = Get-UserSessions
$linusers = $result.User
$monuser = &#39;##WildValue##&#39;

if ($monuser -in $linusers){
    Write-Output &#34;Status=1&#34;    
} else {
    Write-Output &#34;Status=0&#34;
}</value>
